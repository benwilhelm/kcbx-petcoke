#!/usr/bin/env node
var cwd = process.cwd();
require(cwd + "/lib/env");
var constants = require(cwd + "/lib/constants")

var weather = require(cwd + "/services/weatherService")
  , qryLat = constants.KCBX_LAT
  , qryLng = constants.KCBX_LNG
  ;

weather.getForecast(qryLat, qryLng, function(err, fc){
  process.exit();
})
